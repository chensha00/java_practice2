<delete id="delete" parameterType="java.util.Arrays">
    DELETE FROM  order_detail WHERE  id IN
    <if test="@Ognl@isNotEmpty(ids)">
        <foreach collection="ids" item="orderDetailId" open="("
                 separator="," close=")">
            ${testId}
        </foreach>
    </if>
</delete>
</mapper>